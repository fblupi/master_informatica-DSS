         suppressJoinFailure="yes"
         xmlns:tns="http://iberi"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="iberiaArtifacts.wsdl" namespace="http://iberi" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:iberia"
                     myRole="iberiaProvider"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:iberiaRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:iberiaResponseMessage"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in iberia.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:iberia"
                 operation="getPrecio" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:if name="If">
            <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$input.payload/tns:destino = "londres"]]></bpel:condition>
            <bpel:assign validate="no" name="AssignIf">
                <bpel:copy>
                    <bpel:from><bpel:literal xml:space="preserve"><tns:iberiaResponse xmlns:tns="http://iberi" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:precio>60.0</tns:precio>
</tns:iberiaResponse>
</bpel:literal></bpel:from>
                    <bpel:to variable="output" part="payload"></bpel:to>
                </bpel:copy>
                
            </bpel:assign>
            <bpel:elseif>
                <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$input.payload/tns:destino = "chicago"]]></bpel:condition>
                <bpel:assign validate="no" name="AssignElseIf">
                    <bpel:copy>
                        <bpel:from><bpel:literal xml:space="preserve"><tns:iberiaResponse xmlns:tns="http://iberi" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:precio>200.0</tns:precio>
</tns:iberiaResponse></bpel:literal></bpel:from>
                        <bpel:to variable="output" part="payload"></bpel:to>
                    </bpel:copy>
                    
                </bpel:assign>
            </bpel:elseif><bpel:else>
                <bpel:assign validate="no" name="AssignElse">
                    
                    
                    <bpel:copy>
                        <bpel:from><bpel:literal xml:space="preserve"><tns:iberiaResponse xmlns:tns="http://iberi" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:precio>100.0</tns:precio>
</tns:iberiaResponse>
</bpel:literal></bpel:from>
                        <bpel:to variable="output" part="payload"></bpel:to>
                    </bpel:copy>
                    
                </bpel:assign>
            </bpel:else></bpel:if>
        
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:iberia"
               operation="getPrecio" 
               variable="output"
               />
    </bpel:sequence>
